{
  "name": "movinwords",
  "version": "1.0.8",
  "author": "Ignacio 'Nacho' Revuelta - iamnacho.com",
  "description": "Add animation to your words and sentences.",
  "license": "MIT",
  "keywords": [
    "animation",
    "words",
    "text",
    "letters"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "test": "vitest --run --reporter verbose",
    "compile-css": "node-sass package/styles/ -o dist/ --output-style compressed",
    "prebuild": "npm run test && npm run clean && npm run compile-css",
    "build": "rollup -c && node package/preparePackage.js",
    "release": "npm run -s build -s && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && cd dist && npm publish"
  },
  "source": "package/movinwords.js",
  "main": "movinwords.js",
  "module": "movinwords.esm.js",
  "browser": "movinwords.min.js",
  "unpkg": "movinwords.umd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revueltai/movinwords.git"
  },
  "bugs": {
    "url": "https://github.com/revueltai/movinwords/issues"
  },
  "homepage": "https://github.com/revueltai/movinwords",
  "devDependencies": {
    "jsdom": "^20.0.2",
    "node-sass": "^8.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.49.0",
    "rollup-plugin-terser": "^7.0.2",
    "vitest": "^0.25.1"
  }
}
